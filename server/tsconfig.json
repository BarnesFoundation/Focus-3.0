{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"noImplicitAny": false,
		"noEmitOnError": false,
		"noEmit": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "../dist/server",
		"baseUrl": "../server",
		"esModuleInterop": true,
		"jsx": "react",
		"skipLibCheck": true
	},
	"compileOnSave": true,
	"include": [
		"**/*.ts",
		"**/*.tsx"
	],
	"exclude": [
		"../server/test",
		"test",
		"node_modules"
	]
}